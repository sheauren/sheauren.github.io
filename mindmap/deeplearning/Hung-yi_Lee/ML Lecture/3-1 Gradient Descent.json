        "background-color": "#d41dea",
        "expanded": false
    },
    {
        "id": "line_38",
        "parentid": "line_36",
        "topic": "\u03b7^t = \u03b7/\u221a(t+1)",
        "background-color": "#d41dea",
        "expanded": false
    },
    {
        "id": "line_39",
        "parentid": "line_36",
        "topic": "g^t = \u2202L(\u03b8^t)/\u2202w",
        "background-color": "#d41dea",
        "expanded": false
    },
    {
        "id": "line_40",
