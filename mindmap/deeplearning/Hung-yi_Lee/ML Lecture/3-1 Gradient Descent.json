        "expanded": false
    },
    {
        "id": "line_50",
        "parentid": "line_49",
        "topic": "\u03c3^0 = \u221a(g^0)^2",
        "background-color": "#01a6d5",
        "expanded": false
    },
    {
        "id": "line_51",
        "parentid": "line_44",
        "topic": "w^2 = w^1 - \u03b7^1/\u03c3^1 * g^1",
        "background-color": "#d41dea",
        "expanded": false
    },
    {
        "id": "line_52",
        "parentid": "line_51",
        "topic": "\u03c3^1 = \u221a1/2*[(g^0)^2+(g^1)^2]",
        "background-color": "#01a6d5",
        "expanded": false
    },
    {
        "id": "line_53",
        "parentid": "line_44",
        "topic": "w^t+1 = w^t - \u03b7^t/\u03c3^t * g^t",
        "background-color": "#d41dea",
        "expanded": false
    },
    {
        "id": "line_54",
        "parentid": "line_53",
        "topic": "\u03c3^t = \u221a1/t*\u03a3_i=0^t(g^i)^2",
