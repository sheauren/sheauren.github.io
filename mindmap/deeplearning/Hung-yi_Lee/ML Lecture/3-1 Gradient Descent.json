    {
        "id": "line_42",
        "parentid": "line_24",
        "topic": "Adagrad",
        "background-color": "#0fbdff",
        "expanded": false
    },
    {
        "id": "line_43",
        "parentid": "line_42",
        "topic": "divide the learning rate of each parameter by the root mean square of its previous derivatives",
