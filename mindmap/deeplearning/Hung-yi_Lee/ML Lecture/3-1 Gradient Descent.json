    {
        "id": "line_44",
        "parentid": "line_42",
        "topic": "update function:",
        "background-color": "#e3aba7",
        "expanded": false
    },
    {
        "id": "line_45",
        "parentid": "line_44",
        "topic": "w^t+1 <- w^t - \u03b7^t/\u03c3^t * g^t",
        "background-color": "#d41dea",
        "expanded": false
    },
    {
        "id": "line_46",
        "parentid": "line_45",
